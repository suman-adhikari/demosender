    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>

  <connectionStrings>
    <add name="DemoVasup" connectionString="Data Source=SUMAN-PC;Initial Catalog=DemoVasup;User ID=root;Password=root" providerName="System.Data.SqlClient" />
